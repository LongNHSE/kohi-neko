{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "crons": [
    {
      "path": "controllers/cron.js",
      "schedule": "*/46 * * * *"
    },
    {
      "path": "/controllers/cron.js",
      "schedule": "*/46 * * * *"
    },
    {
      "path": "/controllers/cron.js",
      "schedule": "* * * * *"
    }
  ]
}
