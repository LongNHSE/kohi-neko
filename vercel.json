{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/cron.js": {
      "runtime": "nodejs18.x",
      "memory": 3008,
      "schedule": "* * * * *"
    }
  }
}
